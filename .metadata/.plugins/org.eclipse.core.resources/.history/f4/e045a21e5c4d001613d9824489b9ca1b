package pizza;

import java.awt.Graphics;

import javax.swing.JFrame;

public class PizzaMaker {
	
	public static void main(String[] args){
		
		JFrame myFrame = new JFrame();
		
		CheesePizza myPizza = new CheesePizza();
		
		Graphics myg = new Graphics();
		
		
		myPizza.draw();
		
		myFrame.add(myg));
	}
}
